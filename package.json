{
  "name": "ultimate-steels-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "node scripts/show-backup-status.js && vite",
    "dev:strict": "node scripts/show-backup-status.js && node scripts/check-frontend-contract.cjs && vite",
    "build": "vite build",
    "build:ci": "npm run build && npm run ci:guard-bundle",
    "ci:guard-bundle": "bash scripts/ci-guard-contract-bundle.sh",
    "preview": "vite preview",
    "generate-types": "echo 'Types are generated from proto files - see src/types/generated/'",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "npm run typecheck && vitest run --coverage",
    "test:contracts": "vitest run --testPathPattern='contract|schemas'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:staged": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "pre-commit": "npm run lint:fix && npm run typecheck",
    "prepare": "husky",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:forms": "node tests/form-validation/run-all-tests.js",
    "test:form:account-statement": "node tests/form-validation/test-account-statement-form.js",
    "test:form:payment": "node tests/form-validation/test-add-payment-form.js",
    "test:form:reservation": "node tests/form-validation/test-reservation-form.js",
    "test:form:transfer": "node tests/form-validation/test-transfer-form.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@steelapp/contracts": "file:../steelapp-contracts",
    "axios": "^1.10.0",
    "baseline-browser-mapping": "^2.8.32",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.515.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "cypress": "^13.6.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "^24.33.0",
    "rollup": "^4.19.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.10",
    "husky": "^9.1.7"
  }
}
