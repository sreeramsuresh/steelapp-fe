{
  "name": "ultimate-steels-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@10.2.3",
  "scripts": {
    "start": "vite",
    "dev": "node scripts/show-backup-status.js && vite",
    "dev:strict": "node scripts/show-backup-status.js && node scripts/check-frontend-contract.cjs && vite",
    "build": "vite build",
    "build:ci": "npm run build && npm run ci:guard-bundle",
    "ci:guard-bundle": "bash scripts/ci-guard-contract-bundle.sh",
    "preview": "vite preview",
    "generate-types": "echo 'Types are generated from proto files - see src/types/generated/'",
    "typecheck": "tsc --noEmit",
    "test": "node --test 'src/**/__tests__/**/*.test.mjs'",
    "test:watch": "node --test --watch 'src/**/__tests__/**/*.test.mjs'",
    "test:services": "node --test 'src/services/__tests__/**/*.test.mjs'",
    "test:coverage": "node --experimental-coverage --test 'src/**/__tests__/**/*.test.mjs'",
    "test:contracts": "node --test 'src/**/__tests__/**/*contract*.test.mjs'",
    "lint": "biome lint src",
    "lint:ci": "biome lint src",
    "lint:staged": "biome lint --changed",
    "lint:fix": "biome check src --write --vcs-enabled=false",
    "format": "biome format src --write --vcs-enabled=false",
    "pre-commit": "biome check src --write && npm run typecheck",
    "prepare": "husky",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:forms": "node tests/form-validation/run-all-tests.js",
    "test:form:account-statement": "node tests/form-validation/test-account-statement-form.js",
    "test:form:payment": "node tests/form-validation/test-add-payment-form.js",
    "test:form:reservation": "node tests/form-validation/test-reservation-form.js",
    "test:form:transfer": "node tests/form-validation/test-transfer-form.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@steelapp/contracts": "file:../steelapp-contracts",
    "axios": "^1.10.0",
    "baseline-browser-mapping": "^2.8.32",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.515.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@biomejs/biome": "^2.3.13",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "latest",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "cssnano": "^7.1.1",
    "cypress": "^13.6.3",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "^24.33.0",
    "rollup": "^4.19.0",
    "sharp": "^0.34.5",
    "sinon": "^21.0.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "latest"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.27.2",
    "@rollup/rollup-linux-x64-gnu": "^4.57.1"
  }
}
